{
  "name": "hello-rojo",
  "servePort": 8000,
  "partitions": {
    "include": {
			"path": "include",
			"target": "ReplicatedStorage.RobloxTS.Include"
		},
    "modules": {
			"path": "modules",
			"target": "ReplicatedStorage.RobloxTS.Modules"
    },
    "replicated-storage": {
      "path": "dist\\replicated-storage",
      "target": "ReplicatedStorage.Rojo"
    },
    "server-storage": {
      "path": "dist\\server-storage",
      "target": "ServerStorage.Rojo"
    },
    "workspace": {
      "path": "dist\\workspace",
      "target": "Workspace.Rojo"
    }
  }
}
